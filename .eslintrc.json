{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "react",
        "prettier",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-console": "off",
        "react/jsx-filename-extension": "off",
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "all",
                "singleQuote": true,
                "printWidth": 100
            }
        ]
    }
}